#include <stdio.h>

int first_to_repeat(int array[],int n);

int main() {
    int array[7]={3, 12, 1, 12, 7, 7, 12};
    int r=first_to_repeat(array,7);
    printf("the first element to repeat is: %d",r);

    return 0;
}
int first_to_repeat(int array[],int n){
    int repetition;
    int cnt=0;
    for(int i=0; i<n;i++){
        if(array[i]==array[cnt]){
            repetition=array[i];
        }
        cnt++;
    }
    return repetition;
}
the first element to repeat is: 12
