#include <stdio.h>

int main() {
    float array_a[6];
    float array_b[6];
    float array_c[12];
    int i;
    
    printf("Enter elemnts for array c: ");
    for(i=0;i<12;i++){
        scanf("%f",&array_c[i]);
    }
    
    for(i=0;i<6;i++){
        array_a[i]=array_c[2*i];
        array_b[i]=array_c[2*i+1];
    }
    printf("Elements of array a are: ");
    for(i=0;i<6;i++){
        printf("%.1f ", array_a[i]);
    }
    
    printf("\nElements of array b are: ");
    for(i=0;i<6;i++){
        printf("%.1f ", array_b[i]);
    }
   
    return 0;
}

