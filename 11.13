#include <stdio.h>

int uniquenum(int array[], int n);

int main() {
    int array[7] = {3, 12, 1, 12, 7, 3, 12};
    int unique = uniquenum(array, 7);

    if (unique != -1) {
        printf("First unique element is : %d\n", unique);
    } else {
        printf("No unique elements found.\n");
    }

    return 0;
}

int uniquenum(int array[], int n) {
    for (int i = 0; i < n; i++) {
        int cnt = 0;
        for (int j = 0; j < n; j++) {
            if (array[i] == array[j]) {
                cnt++;
            }
        }
        
        if (cnt == 1) {
            return array[i]; 
        }
    }
    return -1; 
}

First unique element is : 1
