#include <stdio.h>

int main() {
    
    int array_a[5];
    int array_b[5];
    int array_c[5];
    int cnt=0;
    
    printf("Enter elements for array a: ");
    for(int i=0;i<5;i++){
        scanf("%d",&array_a[i]);
    }
    printf("Enter elements for array b: ");
    for(int i=0;i<5;i++){
        scanf("%d",&array_b[i]);
    }
    
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
         if(array_a[i]==array_b[j]){
            array_c[cnt]=array_a[i];
                int already_exists = 0;
                for(int k = 0; k < cnt; k++) {
                    if(array_c[k] == array_a[i]) {
                        already_exists = 1;
                        break;
                    }
                }
            if(already_exists==0) {
                    array_c[cnt] = array_a[i];
                    cnt++;
                }
                break;
        }
        }
    }
    printf("elements that are common: ");
    for(int i=0;i<cnt;i++){
        printf("%d ",array_c[i]);
    }

    return 0;
}

