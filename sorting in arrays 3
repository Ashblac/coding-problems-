#include <stdio.h>

int main() {
    int search;
    int j=-1;
    int array[5]={1,2,3,4,5};
    
    printf("Enter the value you want to search for: ");
    scanf("%d",&search);
    
    for(int i=0;i<5;i++){
        if(search==array[i]){
            j==i;
        }
    }
    
    if(j==-1){
        printf("nope");
    }
    else{
        printf("yep");
    }

    return 0;
}
