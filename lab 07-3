#include<stdio.h>
#include<stdlib.h>
int reverse (int n){
int rev = 0;
while(n>0){
rev = rev*10+n%10;
n/=10;
}
return rev;
}
int main(){
int n,rev,diff,steps=0;
 
while(1){
printf("Enter a 2-digit integer:");
scanf("%d",&n);
if(n>=10&&n<=99){
break;
}
else{
printf("ERROR: Please enter a 2-digit integer:");
}
}
 
printf("subtraction chain = %d\n",n);
 
