#include <stdio.h>

int main() {
    float grades[10];
    float avg;
    float sum=0;
    
    printf("Enter the grades ");
    for(int i=0; i<10; i++){
        scanf("%f",& grades[i]);
        sum+=grades[i];
    }
    avg=sum/10;
    for(int i=0; i<10; i++){
    if(grades[i]>avg){
        printf("%.1f ",grades[i]);
    }
    }
    

    return 0;
}
