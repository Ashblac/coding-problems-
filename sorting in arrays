#include <stdio.h>

int main() {
  float grades[5];
  float change;
  
  printf("enter 5 grades: ");
  for(int i=0;i<5;i++){
      scanf("%f",&grades[i]);
  }
  
  for(int j=0;j<5;j++){
      for(int i=0;i<4;i++){
          if(grades[i]>grades[i+1]){
              change=grades[i];
              grades[i]=grades[i+1];
              grades[i+1]=change;
          }
      }
  }
  
  printf("in ascending order: ");
  for(int i=0;i<5;i++){
      printf("%.2f",grades[i]);
  }

    return 0;
}
