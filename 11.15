#include <stdio.h>

int main() {
    
    int array_1[5];
    int array_2[5];
    int array_3[5];
    
    printf("Enter elements for the first array: ");
    for(int i=0;i<5;i++){
        scanf("%d",&array_1[i]);
    }
    printf("Enter elements for the second array: ");
    for(int i=0;i<5;i++){
        scanf("%d",&array_2[i]);
    }
    
    for(int i=0;i<5;i++){
        array_3[i]=array_2[i]+array_1[i];
    }
    printf("elements of the third array: ");
    for(int i=0;i<5;i++){
        printf("%d ",array_3[i]);
    }

    return 0;
}
