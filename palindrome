#include <stdio.h>

int main() {
    
    int array_a[10];
    
    printf("Enter elements for array a: ");
    for(int i=0;i<10;i++){
        scanf("%d",&array_a[i]);
    }
    for(int i=0;i<10;i++){
        array_a[i]=array_a[9-i];
    }
    printf("Enter elements for swapped array: ");
    for(int i=0;i<10;i++){
        printf("%d ",array_a[i]);
    }

    return 0;
}
