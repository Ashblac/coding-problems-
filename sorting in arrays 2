#include <stdio.h>

int main() {
  float grades[10];
  float change;
  
  printf("enter 10 grades: ");
  for(int i=0;i<10;i++){
      scanf("%f",&grades[i]);
  }
  
  for(int j=1;j<5;j++){
      for(int i=0;i<4;i++){
          if(grades[i]>grades[i+1]){
              change=grades[i];
              grades[i]=grades[i+1];
              grades[i+1]=change;
          }
      }
  }
  
  printf("highest 5: \n");
  for(int i=5;i<10;i++){
      printf("%.2f\n",grades[i]);
  }
  printf("lowest 5: \n");
  for(int i=0;i<5;i++){
      printf("%.2f\n",grades[i]);
  }

    return 0;
}
