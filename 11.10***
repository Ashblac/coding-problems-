#include <stdio.h>

int main() {
    int car[50];
    int light[50];
    int light_state[50]={0};
    int i;
    for(i=0;i<50;i++){
        car[i]=i+1;
        light[i]=i+1;
    }
    for(i=0;i<50;i++){
        for(int j=0;j<10;j++){
            if(car[j]%light[i]==0){
            light_state[i] = !light_state[i];
        }
        }
    }
    printf("LIGHTS THAT ARE ON:\n");
    for(i = 0; i < 50; i++) {
        if(light_state[i] == 1) {
            printf("%d, ", i + 1);
        }
    }

    printf("\n\nLIGHTS THAT ARE OFF:\n");
    for(i = 0; i < 50; i++) {
        if(light_state[i] == 0) {
            printf("%d, ", i + 1);
        }
    }
    
    return 0;




    #include <stdio.h>

int main() {
    int array_a[5];
    int array_b[5];
    int i, j, k;

    printf("Enter 5 elements for array a: ");
    for(i = 0; i < 5; i++) scanf("%d", &array_a[i]);

    printf("Enter 5 elements for array b: ");
    for(i = 0; i < 5; i++) scanf("%d", &array_b[i]);

    printf("Elements that are common in a&b: ");

    for(i = 0; i < 5; i++) {
        // --- DUPLICATE CHECK ---
        // Check if we already processed this specific number in array_a
        int already_seen = 0;
        for(k = 0; k < i; k++) {
            if(array_a[i] == array_a[k]) {
                already_seen = 1;
                break;
            }
        }

        // If we haven't seen this number before, check it against array_b
        if(!already_seen) {
            for(j = 0; j < 5; j++) {
                if(array_a[i] == array_b[j]) {
                    printf("%d ", array_a[i]);
                    break; // Move to the next number in array_a
                }
            }
        }
    }

    printf("\n");
    return 0;
}
}

#include<stdio.h>
int main()
{
    int lamps[50];
    int cars[10];
    int result[50]={0};
    int i;
    int j;
    for(i=0; i<50; i++){
        lamps[i]=i+1;
    }
    for(i=0; i<10; i++){
        cars[i]=i+1;
    }
    for(i=0; i<50; i++){
        for(j=0; j<10; j++){
            if(lamps[i]%cars[j]==0){
                if(result[i]==0){
                    result[i]=1;
                }
                else{
                    result[i]=0;
                }
            }
        }
    }
    printf("The opened lamps:\n");
    for(i=0; i<50; i++){
        if(result[i]==1){
            printf("%d, ", i+1);
        }
    }
    printf("\nThe closed lamps:\n");
    for(i=0; i<50; i++){
        if(result[i]==0){
            printf("%d, ", i+1);
        }
    }
    return 0;
}
