#include <stdio.h>
int find_min(int array[10]){
    int min;
    min=array[0];
    for(int j=0;j<10;j++){
    if(array[j]<min){
        min=array[j];
    }
}
return min;
}
int main() {
    int array[10];
    int min;
    printf("Enter elements for the array: ");
    
    for(int i=0;i<10;i++){
        scanf("%d",&array[i]);
    }
    min= find_min(array);
    printf("%d",min);
    return 0;
}

