#include <stdio.h>

int main() {
    int array[]={1,2,3,4,5};
    int i;/*index*/
    int max=array[0];
    int location;
    int size= sizeof(array)/ sizeof(array);
    for(i=0; i<= size;i++){
        if(array[i]>max){
            max=array[i];
            location = i;
        }
    }
    printf("The max number in this array is %d with location %d",max,location);

    return 0;
}
