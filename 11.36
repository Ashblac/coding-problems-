#include <stdio.h>

int main() {
    
    int array_a[10];
    int temp;
    printf("Enter elements for array a: ");
    for(int i=0;i<10;i++){
        scanf("%d",&array_a[i]);
    }
    for(int i=0;i<5;i++){
        temp = array_a[i];            
        array_a[i] = array_a[9-i];   
        array_a[9-i] = temp;
    }
    printf("elements of the swapped array: ");
    for(int i=0;i<10;i++){
        printf("%d ",array_a[i]);
    }

    return 0;
}
Enter elements for array a: 4 5 6 2 3 1 8 9 2 10
elements of the swapped array: 10 2 9 8 1 3 2 6 5 4 
