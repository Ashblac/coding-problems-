(corrected)
#include <stdio.h>

int distinctIndex(int array[], int n);

int main() {
    int array[7] = {4, 7, 4, 5, 3, 7, 3};
    int index = distinctIndex(array, 7);

    if (index != -1) {
        printf("First unique element is at index: %d\n", index);
    } else {
        printf("No unique elements found.\n");
    }

    return 0;
}

int distinctIndex(int array[], int n) {
    for (int i = 0; i < n; i++) {
        int cnt = 0;
        for (int j = 0; j < n; j++) {
            if (array[i] == array[j]) {
                cnt++;
            }
        }
        // If cnt is exactly 1, it means the element only matched itself
        if (cnt == 1) {
            return i; // Returning the 0-based index
        }
    }
    return -1; // Return -1 if no unique element exists
}
